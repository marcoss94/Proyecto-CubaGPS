    <script>
        $('#wrongDate').hide;
        $(document).ready( function() {
            $('#inputEntrada').attr('min',yyyymmdd());
            $('#inputSalida').attr('min',yyyymmdd_salida());
            $('#inputEntrada').val(yyyymmdd());
            $('#inputSalida').val(yyyymmdd_salida());
            setPrice();
            $('#inputSalida').on('change',function () {
                setPrice();
            })
            $('#inputEntrada').on('change',function () {
                setPrice();
            })
            $('.form-check-input').click(function () {
                setPrice()
            })
        });

        function setPrice(){
            var precio=0;
            $('input:checkbox').filter(':checked').each(function () {
                precio+=parseInt(this.alt);
            })
            var entrada = new Date($('#inputEntrada').val());
            var salida = new Date($('#inputSalida').val());
            var dias = salida-entrada;
            dias= parseInt(dias)/86400000;
            if(dias==0)dias=1;
            if(dias<0)dias=0;
            document.getElementById('precio').innerHTML = '‚Ç¨'+dias*precio ;
        }
        var carousel = $(".carouselCarucel"),
            currdeg = 0;
        $(".nextCarucel").on("click", {d: "n"}, rotate);
        $(".prevCarucel").on("click", {d: "p"}, rotate);
        function rotate(e) {
            if (e.data.d == "n") {
                currdeg = currdeg - 60;
            }
            if (e.data.d == "p") {
                currdeg = currdeg + 60;
            }
            carousel.css({
                "-webkit-transform": "rotateY(" + currdeg + "deg)",
                "-moz-transform": "rotateY(" + currdeg + "deg)",
                "-o-transform": "rotateY(" + currdeg + "deg)",
                "transform": "rotateY(" + currdeg + "deg)"
            });
        }
        $('#reserveButton').click(function () {
            var entrada = new Date($('#inputEntrada').val());
            var salida = new Date($('#inputSalida').val());
            var cantA=new Number($('#cantA').val());
            var cantN=new Number($('#cantN').val());
            var capacidad = 0;
            $('input:checkbox').filter(':checked').each(function () {
                capacidad+=parseInt($(this).val());
            })
            if (salida < entrada) {
                var entrada = new Date($('#inputEntrada').val());
                var salida = new Date($('#inputSalida').val());
                if (salida < entrada) {
                    var text = ($('#lang').val() == 'es') ? 'La fecha de salida no puede ser antes que la fecha de entrada'
                        : 'The departure date can not be before the date of entry';
                    tpl = '<div class="alert alert-danger">' + text + '</div>';
                    $('#errorBox').html(tpl)
                    return;
                }
            }
            if ($('input:checkbox').filter(':checked').length < 1) {
                var text = ($('#lang').val() == 'es') ? 'Debe seleccionar al menos una habitacion'
                    : 'You must select at least one room';
                tpl = '<div class="alert alert-danger">' + text + '</div>';
                $('#errorBox').html(tpl)
                return;
            }
            if (salida.toTimeString() == 'Invalid Date' || entrada.toTimeString() == 'Invalid Date') {
                var text = ($('#lang').val() == 'es') ? 'Debe seleccionar las fechas de entrada y salida'
                    : 'You must select the entry and departure dates';
                tpl = '<div class="alert alert-danger">' + text + '</div>';
                $('#errorBox').html(tpl)
                return;
            }
            if(capacidad<cantN+cantA){
                var text = ($('#lang').val() == 'es') ? 'La cantidad de personas excede la capacidad de las habitaciones seleccionadas'
                    : 'The amount of people exceeds the capacity of the selected rooms';
                tpl = '<div class="alert alert-danger">' + text + '</div>';
                $('#errorBox').html(tpl)
                return;
            }

            $('#reserveForm').submit();


        })
        $('#createCommentButton').click(function () {
            var objectId = $('#objectId').val();
            var objectType = $('#objectType').val();
            var comment = $('#commentArea').val();
            var userId = $('#userId').val();
            if (comment == '') {
                alert('debe escribir un comentario');
            } else {
                var data = {targetId: objectId, type: objectType, text: comment, userId: userId}
                $.ajax({
                    type: "POST",
                    url: "{{ path('ajax_create_comment') }}", // call the php file ajax/tuto-autocomplete.php (check the routine we defined)
                    data: data,
                    success: function (response) {
                        if (response.result) {
                            alert('Su comentario ha sido recivido correctamente');
                            $('#commentArea').val('');
                        }
                    }
                })
                ;
            }
        })

    </script>

