                                                    <i class="fas fa-star star"></i>
                                                {% endfor %}
                                            {% endif %}
                                            {% if object.valoracionArray[1] %}
                                                <i class="fas fa-star-half-alt star"></i>
                                            {% endif %}
                                            {% if object.valoracionArray[2]!=0 %}
                                                {% for j in 1..object.valoracionArray[2] %}
                                                    <i class="far fa-star star"></i>
                                                {% endfor %}
                                            {% endif %}
                                        </div>
                                        <div class="form-group col-md-12">
                                            <label class="text-muted">{{ app.session.get('language') == 'es'? 'Cantidad de Adultos':'Adults' }}</label>
                                            <select class="custom-select" id="cantA" name="cantA">
                                                {% for number in 1..object.capacidad %}
                                                    <option value="{{ number }}" {% if number==1 %} selected {% endif %}>{{ number }}</option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div class="form-group col-md-12">
                                            <label class="text-muted"
                                                   for="inputAge">{{ app.session.get('language') == 'es'? 'Cantidad de Niños':'Number of children' }}</label>
                                            <label class="text-muted"
                                                   for="inputAge">{{ app.session.get('language') == 'es'? 'Hasta 12 años':'up to 12 years' }}</label>
                                            <select class="custom-select" id="cantN" name="cantN">
                                                {% for number in 0..(object.capacidad-1) %}
                                                    <option value="{{ number }}" {% if number==0 %} selected {% endif %}>{{ number }}</option>
                                                {% endfor %}
                                            </select>

                                        </div>
                                        <div class="form-group col-md-12">
                                            <label class="text-muted"
                                                   for="inputAge">{{ app.session.get('language') == 'es'? 'Habitaciones':'Rooms' }}</label>
                                            <div class="form-group">
                                                {% for h in object.habitaciones %}
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox"
                                                               value="{{ h.capacidad }}"
                                                               name="{{ h.id }}" alt="{{ h.precio }}"
                                                               id="{{ loop.index0 }}">
                                                        <label class="form-check-label" for="{{ loop.index }}">
                                                            {{ app.session.get('language') == 'es'? h.nombre : h.name }}
                                                        </label>
                                                    </div>
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div class="form-group col-md-12">
                                            <label class="text-muted"
                                                   for="inputEntrada">{{ app.session.get('language') == 'es'? 'Entrada':'Entrance' }}</label>
                                            <input type="date" class="form-control" name="entrada" id="inputEntrada"
                                                   min="2017-01-01"
                                                   required>
                                        </div>
                                        <div class="form-group col-md-12">
                                            <label class="text-muted"
                                                   for="inputSalida">{{ app.session.get('language') == 'es'? 'Salida':'Exit' }}</label>
                                            <input type="date" class="form-control" name="salida" id="inputSalida"
                                                   min="2017-01-01"
                                                   required>
                                        </div>
                                    </div>
                                    {% if app.user %}
                                        <div class="form-row">
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Email</label>
                                                <input type="email" class="form-control" id="inputEail1"
                                                       aria-describedby="emailHelp" name="email" required="required"
                                                       placeholder="Enter email" value="{{ app.user.email }}">
                                                <small id="emailHelp"
                                                       class="form-text text-muted">{{ app.session.get('language') == 'es'?
                                                    'Te contactaremos a travéz de esta dirección de correo': 'We will contact you through this email address' }}</small>
                                            </div>
                                        </div>
                                    {% endif %}
                                </div>
                                <div id="errorBox"></div>
                                <div class="modal-footer">
                                    <a href="{{ path('reservacion') }}">Reservacion </a>
                                    <label id="precio"></label>
                                    <button type="button" id="reserveButton"
                                            class="btn btn-primary">{{ app.session.get('language') == 'es'? 'Reservar':'Reserve' }}</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                {#---------------------------------Formulario de Reserva-----------------------------------------------------#}
            </div>
        </div>
    </section>



