                                                    {% for image in i.images %}
                                                        <div {% if loop.index==1 %}class="carousel-item active"
                                                             {% else %}class="carousel-item"{% endif %}>
                                                            <img class="img-fluid fotosOferta"
                                                                 src="{{ asset(image.half) }}"
                                                                 alt="{{ image.altName }}">
