        });
        function yyyymmdd() {
            var today = new Date();
            var now = new Date();
            now.setDate(today.getDate() + 4);
            var y = now.getFullYear();
            var m = now.getMonth() + 1;
            var d = now.getDate();
            var mm = m < 10 ? '0' + m : m;
            var dd = d < 10 ? '0' + d : d;
            return '' + y + '-' + mm + '-' + dd;
        }
        function yyyymmdd_salida() {
            var today = new Date();
            var now = new Date();
            now.setDate(today.getDate() + 5);
            var y = now.getFullYear();
            var m = now.getMonth() + 1;
            var d = now.getDate();
            var mm = m < 10 ? '0' + m : m;
            var dd = d < 10 ? '0' + d : d;
            return '' + y + '-' + mm + '-' + dd;
        }
    </script>

    <script type="text/javascript">
        $(function () {
            var $tabButtonItem = $('#tab-button li'),
                $tabSelect = $('#tab-select'),
                $tabContents = $('.tab-contents'),
                activeClass = 'is-active';

            $tabButtonItem.first().addClass(activeClass);
            $tabContents.not(':first').hide();

            $tabButtonItem.find('a').on('click', function (e) {
                var target = $(this).attr('href');

                $tabButtonItem.removeClass(activeClass);
                $(this).parent().addClass(activeClass);
                $tabSelect.val(target);
                $tabContents.hide();
                $(target).show();
                e.preventDefault();
            });

            $tabSelect.on('change', function () {
                var target = $(this).val(),
                    targetSelectNum = $(this).prop('selectedIndex');

                $tabButtonItem.removeClass(activeClass);
                $tabButtonItem.eq(targetSelectNum).addClass(activeClass);
                $tabContents.hide();
                $(target).show();
            });
        });
        function show_alert_box(type, head, body) {
            if (type == "warning") {
                $("#textoWarningGrande").text(head);
                $("#textoWarning").text(body);
                $('.Alertwarning').animate({top: "0"}, 500);
                $('.Alertwarning').css("display", "block");
                $('#messageArea').val('');
            } else if (type == "error") {
                $("#textoErrorGrande").text(head);
                $("#textoError").text(body);
                $('.Alerterror').animate({top: "0"}, 500);
                $('.Alerterror').css("display", "block");
                $('#messageArea').val('');
            } else {
                $("#textoSuccessGrande").text(head);
                $("#textoSuccess").text(body);
                $('.Alertsuccess').animate({top: "0"}, 500);
                $('.Alertsuccess').css("display", "block");
                $('#messageArea').val('');
            }
        }

    </script>




