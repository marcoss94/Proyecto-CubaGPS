            } else if (!email.match(emailPattern)) {
                var head = '{{ app.session.get('language') == 'es'? 'Lo sentimos':'Sorry' }}';
                var body = '{{ app.session.get('language') == 'es'?
                'Para contactarnos es necesario que insertes tu número de correo electrónico'
                :'To contact us you need to insert your email' }}';
                show_alert_box("error", head, body);
            } else if (!tel.match(telPattern)) {//
                var head = '{{ app.session.get('language') == 'es'? 'Lo sentimos':'Sorry' }}';
                var body = '{{ app.session.get('language') == 'es'?
