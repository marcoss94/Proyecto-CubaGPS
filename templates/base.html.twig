{% block footer %}
    <!-- Contact -->

    {#<div class="popupunder alert alert-success fade in">#}
    {#<button type="button" class="close close-sm" data-dismiss="alert"><i#}
    {#class="glyphicon glyphicon-remove"></i></button>#}
    {#<strong>Success : </strong> The update process has been completed successfull!#}
    {#</div>#}

    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading text-uppercase">{{ app.session.get('language') == 'es'? 'Contáctenos':'Contact Us' }}</h2>
                    <h3 class="section-subheading text-muted">{{ app.session.get('language') == 'es'? 'Comuniquese con nosotros, trabajamos para usted ':'Contact us, we work for you' }}</h3>


                    <div class="Alertinfo message" style="display: none">
                        <h3 id="textoInfoGrande">FYI, something just happened!</h3>
                        <p id="textoInfo">This is just an info notification message.</p>
                    </div>

                    <div class="Alerterror message" style="display: none">
                        <h3 id="textoErrorGrande">Ups, an error ocurred</h3>
                        <p id="textoError">This is just an error notification message.</p>

                    </div>

                    <div class="Alertwarning message" style="display: none">
                        <h3 id="textoWarningGrande">Wait, I must warn you!</h3>
                        <p id="textoWarning">This is just a warning notification message.</p>
                    </div>

                    <div class="Alertsuccess message" style="display: none">
                        <h3 id="textoSuccessGrande">Congrats, you did it!</h3>
                        <p id="textoSuccess">This is just a success notification message.</p>

                    </div>

                    {#<ul id="trigger-list">#}
                    {#<li><a href="#b" class="trigger Alertinfo-trigger">Info</a></li>#}
                    {#<li><a href="#b" class="trigger Alerterror-trigger">Error</a></li>#}
                    {#<li><a href="#b" class="trigger Alertwarning-trigger">Warning</a></li>#}
                    {#<li><a href="#b" class="trigger Alertsuccess-trigger">Success</a></li>#}
                    {#</ul>#}


                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <input class="form-control" id="name" type="text"
                                       placeholder="{{ app.session.get('language') == 'es'? 'Tu Nombre':'Your Name' }}*"
                                       required="required" value="{% if app.user %}{{ app.user }}{% endif %}"
                                       data-validation-required-message="{{ app.session.get('language') == 'es'? 'Por favor entre su nombre.':'Please enter your name.' }}">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="email" type="email"
                                       placeholder="{{ app.session.get('language') == 'es'? 'Tu Correo':'Your Email' }} *"
                                       required="required" value="{% if app.user %}{{ app.user.email }}{% endif %}"
                                       data-validation-required-message="{{ app.session.get('language') == 'es'? 'Por favor entre su correo.':'Please enter your email address.' }}">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="phone" type="tel"
                                       placeholder="{{ app.session.get('language') == 'es'? 'Tu Teléfono':'Your Phone' }} *"
                                       required="required number"
                                       data-validation-required-message="{{ app.session.get('language') == 'es'? 'Por favor entre su número de teléfono':'Please enter your phone number.' }}">
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <textarea class="form-control" id="messageArea"
                                          placeholder="{{ app.session.get('language') == 'es'? 'Tu Mensaje':'Your Message' }} *"
                                          required="required"
                                          data-validation-required-message="{{ app.session.get('language') == 'es'? 'Por favor entre su mensaje':'Please enter a message.' }}"></textarea>
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <div class="col-lg-12 text-center">
                            <div id="success"></div>
                            <button id="createContact" class="btn btn-primary btn-xl text-uppercase" style="background-color: #f9b501;
border-color: #f9b501">
                                {{ app.session.get('language') == 'es'? 'Enviar mensaje':'Send Menssage' }}
                            </button>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

