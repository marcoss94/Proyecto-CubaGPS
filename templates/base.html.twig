                                   href="#paquetes">{{ app.session.get('language') == 'es'? 'Paquetes':'Packages' }}</a>
                                <a class="nav-link js-scroll-trigger "
                                   href="#carros">{{ app.session.get('language') == 'es'? 'Coches':'Cars' }}</a>
                                <a class="nav-link js-scroll-trigger "
                                   href="#excursiones">{{ app.session.get('language') == 'es'? 'Excursiones':'Trips' }}</a>


                                {#<a class="nav-link js-scroll-trigger "#}
                                {#href="">{{ app.session.get('language') == 'es'? 'Casas':'Houses' }}</a>#}
                                {#<a class="nav-link js-scroll-trigger "#}
                                {#href="">{{ app.session.get('language') == 'es'? 'Hoteles':'Hotels' }}</a>#}
                                {#<a class="nav-link js-scroll-trigger "#}
                                {#href="">{{ app.session.get('language') == 'es'? 'Paquetes':'Packages' }}</a>#}
                                {#<a class="nav-link js-scroll-trigger "#}
                                {#href="">{{ app.session.get('language') == 'es'? 'Carros':'Cars' }}</a>#}
                                {#<a class="nav-link js-scroll-trigger "#}
                                {#href="">{{ app.session.get('language') == 'es'? 'Excursiones':'Trips' }}</a>#}

                            </div>
                        </div>
                    </li>

                    <li class="nav-item">
                        <div class="dropdown">
                            <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                               data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-globe" aria-hidden="true"></i>
                                <span class="caret"></span>
                            </a>

                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink"
                                 style="background-color: #212529;">

                                <a {% if app.session.get('language') == 'es' %} class="nav-link disabled" {% else %} class="nav-link " href="{{ path('change_language') }}"{% endif %}>Español</a>
                                <a {% if app.session.get('language') == 'en' %} class="nav-link disabled" {% else %} class="nav-link " href="{{ path('change_language') }}"{% endif %}>English</a>

                            </div>
                        </div>
                    </li>


                    {% if app.user %}
                        {% if cart and reserves %}
                            cart and reserves
                            <div class="dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                   data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fas fa-shopping-cart" style="color: red"></i>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink"
                                     style="background-color: #212529;">
                                    <a class="nav-link"
                                       href="{{ path('show_confirmed_reserves') }}"
                                       style="color: red">{{ app.session.get('language') == 'es'? 'Reservas Pendientes':'Pending Reserves' }}</a>
                                    <a class="nav-link"
                                       href="#"
                                       style="color: green">{{ app.session.get('language') == 'es'? 'Reservas Activas':'Active Reserves' }}</a>
                                </div>
                            </div>
                        {% elseif cart %}
                            only cart
                            <li class="nav-item">
                                <a class="nav-link" style="color: red" href="{{ path('show_confirmed_reserves') }}">
                                    <i class="fas fa-shopping-cart"></i>
                                </a>
                            </li>
                        {% elseif reserves %}
                            only reserves
                            <li class="nav-item">
                                <a class="nav-link" style="color: red" href="#">
                                    <i class="fas fa-shopping-cart"></i>
                                </a>
                            </li>
                        {% endif %}

                        <li class="nav-item">
                            <div class="dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                   data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    {#<img src="{{ asset(app.user.picture) }}"#}
                                    <img src="{{ asset(app.user.picture) }}"
                                         class="img-fluid rounded-circle avat"
                                         style=" height: 35px; border-bottom-left-radius: 50%;
                                    border-bottom-right-radius: 50%;width: 35px;" alt="avatar image">
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink"
                                     style="background-color: #212529;">
                                    <a class="nav-link"
                                       href="{{ path('security_logout') }}">{{ app.session.get('language') == 'es'? 'Cerrar Sesión':'Logout' }}</a>
                                    {% if is_granted('ROLE_PUBLISHER') %}
                                        <a class="nav-link js-scroll-trigger" href="" data-toggle="modal"
                                           data-target="#linkModal">
                                            {{ app.session.get('language') == 'es'? 'Enlace Único':'Unic Link' }}
                                        </a>
                                    {% endif %}
                                </div>
                            </div>
                        </li>

                    {% else %}
                        <li class="nav-item">
                            <a class="nav-link js-scroll-trigger" href="" data-toggle="modal" data-target="#loginModal">
                                {{ app.session.get('language') == 'es'? 'Iniciar Sesión':'Login' }}
                            </a>
                        </li>
                    {% endif %}


                </ul>
            </div>
        </div>
    </nav>
{% endblock %}

