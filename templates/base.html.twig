                <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav text-uppercase ml-auto" style="line-height: 2.5;">

                    <li class="nav-item">
                        <form class="form-inline nav-link" action="{{ path('generic_search') }}">
                            <div class="col-auto">
                                <div class="input-group ">
                                    <input type="text" class="form-control" name="searchText" required
                                           id="inlineFormInputGroup"
                                           aria-label="Search"
                                           placeholder="{{ app.session.get('language') == 'es'? 'Buscar':'Search' }}">
                                    <div class="input-group-prepend" style="display: inline">
                                        <div class="input-group-text" style="border: 0;background-color: inherit">
                                            <a class=" js-scroll-trigger" href="" data-toggle="modal"
                                               data-target="#searchModal" style="color: white;">
                                                <i class="fa fa-plus" aria-hidden="true"></i>
                                            </a>
                                        </div>
                                    </div>
                                    <button class="btn btn-primary " type="submit"
                                            style="background-color: #f9b501; border-color: #f9b501;">
                                        <i class="fa fa-search" aria-hidden="true"></i>
                                    </button>
                                </div>
                            </div>
                        </form>
                    </li>

                    <li class="nav-item">
                        <div class="dropdown">
                            <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                               data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                {{ app.session.get('language') == 'es'? 'Servicios':'Services' }}
                            </a>

                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink"
                                 style="background-color: #212529;">

                                {#aki va si estas en el base se bone base true y si no base false#}


                                <a class="nav-link js-scroll-trigger "
                                   href="#casas">{{ app.session.get('language') == 'es'? 'Casas':'Houses' }}</a>
                                {#<a class="nav-link js-scroll-trigger "#}
                                {#href="#hoteles">{{ app.session.get('language') == 'es'? 'Hoteles':'Hotels' }}</a>#}
